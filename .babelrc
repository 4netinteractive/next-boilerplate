{
	"presets": [
		"next/babel",
	],
	"plugins": [
		[
			"styled-components",
			{
				"ssr": true,
				"displayName": true,
				"preprocess": false
			}
		],
		[
			"module-resolver",
			{
				"root": [
					"./"
				],
				"alias": {
					"@Actions": "./src/Actions",
					"@Components": "./src/Components",
					"@Definitions": "./src/Definitions",
					"@Interfaces": "./src/Interfaces",
					"@Reducers": "./src/Redux/Reducers",
					"@Redux": "./src/Redux",
					"@Services": "./src/Services",
					"@Themes": "./src/Themes"
				}
			}
		]
	]
}